{
  "name": "kamplex-api",
  "version": "0.0.1",
  "description": "REST API media streaming",
  "author": "Kaigonia <ktt45678@gmail.com> (https://github.com/ktt45678)",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@azure/storage-blob": "12.17.0",
    "@fastify/cookie": "9.3.1",
    "@fastify/multipart": "8.1.0",
    "@fastify/static": "6.12.0",
    "@nestjs/axios": "3.0.1",
    "@nestjs/bullmq": "10.0.1",
    "@nestjs/cache-manager": "2.2.0",
    "@nestjs/common": "10.3.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.0",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mapped-types": "2.0.4",
    "@nestjs/mongoose": "10.0.2",
    "@nestjs/platform-fastify": "10.3.0",
    "@nestjs/platform-socket.io": "10.3.0",
    "@nestjs/schedule": "4.0.0",
    "@nestjs/swagger": "7.2.0",
    "@nestjs/websockets": "10.3.0",
    "@socket.io/redis-adapter": "8.2.1",
    "@tirke/node-cache-manager-ioredis": "3.6.0",
    "bcrypt": "5.1.1",
    "bullmq": "4.17.0",
    "cache-manager": "5.3.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "fast-average-color": "9.4.0",
    "fastify": "4.25.1",
    "form-data": "4.0.0",
    "ioredis": "5.3.2",
    "iso-639-1": "3.1.0",
    "lodash": "4.17.21",
    "mongoose": "7.6.8",
    "nanoid": "3.3.7",
    "nodejs-snowflake": "2.0.1",
    "p-limit": "3.1.0",
    "parse-redis-url-simple": "1.0.2",
    "reflect-metadata": "0.1.14",
    "remove-accents": "0.5.0",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1",
    "sharp": "0.32.6",
    "slugify": "1.6.6",
    "stream-mmmagic": "2.3.0",
    "validator": "13.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.0",
    "@nestjs/schematics": "10.1.0",
    "@nestjs/testing": "10.3.0",
    "@types/bcrypt": "5.0.2",
    "@types/busboy": "1.5.3",
    "@types/cache-manager": "4.0.6",
    "@types/cache-manager-ioredis": "2.0.6",
    "@types/jest": "29.5.11",
    "@types/lodash": "4.14.202",
    "@types/mime-types": "2.1.4",
    "@types/node": "18.19.8",
    "@types/supertest": "2.0.16",
    "@types/validator": "13.11.8",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "prettier": "3.2.4",
    "supertest": "6.3.4",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  },
  "optionalDependencies": {
    "bufferutil": "4.0.8",
    "utf-8-validate": "6.0.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
