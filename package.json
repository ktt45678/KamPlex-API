{
  "name": "kamplex-api",
  "version": "0.0.1",
  "description": "REST API media streaming",
  "author": "Kaigonia <ktt45678@gmail.com> (https://github.com/ktt45678)",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "0.0.2",
    "@nestjs/bull": "^0.4.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.0.1",
    "@nestjs/platform-fastify": "^8.0.6",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.0.9",
    "bcrypt": "^5.0.1",
    "bull": "^3.29.2",
    "cache-manager": "^3.4.4",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "fastify": "^3.22.0",
    "fastify-multipart": "^5.2.1",
    "fastify-swagger": "^4.11.0",
    "form-data": "^4.0.0",
    "iso-639-1": "^2.1.9",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.13",
    "nanoid": "^3.1.25",
    "node-vibrant": "^3.2.1-alpha.1",
    "nodejs-snowflake": "^1.6.2",
    "probe-image-size": "^7.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.0",
    "stream-mmmagic": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.6",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.5",
    "@types/busboy": "^0.3.1",
    "@types/cache-manager": "^3.4.2",
    "@types/cache-manager-ioredis": "^2.0.2",
    "@types/ioredis": "^4.27.2",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^17.0.0",
    "@types/probe-image-size": "^7.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.1.1",
    "prettier": "^2.4.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
