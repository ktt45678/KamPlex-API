{
  "name": "kamplex-api",
  "version": "0.0.1",
  "description": "REST API media streaming",
  "author": "Kaigonia <ktt45678@gmail.com> (https://github.com/ktt45678)",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@azure/storage-blob": "12.11.0",
    "@fastify/cookie": "7.2.0",
    "@fastify/multipart": "7.1.0",
    "@fastify/static": "6.4.0",
    "@nestjs/axios": "0.1.0",
    "@nestjs/bull": "0.6.0",
    "@nestjs/common": "9.0.4",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.0.4",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "9.2.0",
    "@nestjs/platform-fastify": "9.0.4",
    "@nestjs/platform-socket.io": "9.0.4",
    "@nestjs/schedule": "2.1.0",
    "@nestjs/swagger": "6.0.4",
    "@nestjs/websockets": "9.0.4",
    "@socket.io/redis-adapter": "7.2.0",
    "bcrypt": "5.0.1",
    "bull": "4.8.4",
    "cache-manager": "4.1.0",
    "cache-manager-ioredis": "2.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "fast-average-color": "9.1.1",
    "fastify": "4.2.1",
    "form-data": "4.0.0",
    "iso-639-1": "2.1.15",
    "lodash": "4.17.21",
    "mongoose": "6.4.5",
    "nanoid": "3.3.4",
    "nodejs-snowflake": "2.0.1",
    "p-limit": "3.1.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.6",
    "sharp": "0.30.7",
    "slugify": "1.6.5",
    "stream-mmmagic": "2.3.0",
    "validator": "13.7.0"
  },
  "devDependencies": {
    "@nestjs/cli": "9.0.0",
    "@nestjs/schematics": "9.0.1",
    "@nestjs/testing": "9.0.4",
    "@types/bcrypt": "5.0.0",
    "@types/bull": "3.15.8",
    "@types/busboy": "1.5.0",
    "@types/cache-manager": "4.0.1",
    "@types/cache-manager-ioredis": "2.0.2",
    "@types/jest": "28.1.6",
    "@types/lodash": "4.14.182",
    "@types/mime-types": "2.1.1",
    "@types/node": "18.0.6",
    "@types/sharp": "0.30.4",
    "@types/supertest": "2.0.12",
    "@types/validator": "13.7.4",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "prettier": "2.7.1",
    "supertest": "6.2.4",
    "ts-jest": "28.0.7",
    "ts-loader": "9.3.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "4.7.4"
  },
  "optionalDependencies": {
    "bufferutil": "4.0.6",
    "utf-8-validate": "5.0.9"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
